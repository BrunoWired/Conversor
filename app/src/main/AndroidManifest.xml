<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="br.com.rotapublicitaria.conversor" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Principal"
            android:label="@string/title_activity_principal" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Sobre"
            android:label="@string/title_activity_sobre" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="br.com.rotapublicitaria.conversor.Principal" />
        </activity>
    </application>

</manifest>
